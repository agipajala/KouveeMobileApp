<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kouvee">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_app_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ZoomImage"></activity>
        <activity android:name=".Activity.Ukuran.editUkuran" />
        <activity android:name=".Views.Ukuran.showUkuran" />
        <activity android:name=".Views.Ukuran.UkuranAdapter" />
        <activity android:name=".Activity.Ukuran.createUkuran" />
        <activity android:name=".Activity.Ukuran.MenuUkuran" />
        <activity android:name=".Activity.Produk.editProduk" />
        <activity android:name=".Activity.Produk.detailProduk" />
        <activity android:name=".Activity.Produk.createProduk" />
        <activity android:name=".Views.Produk.ProdukAdapter" />
        <activity android:name=".Views.Produk.showProduk" />
        <activity android:name=".Activity.Produk.MenuProduk" />
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>